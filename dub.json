{
	"name": "swiftover",
	"description": "Swift coordinate liftover",
	"authors": ["James S Blachly, MD"],
	"homepage": "http://github.com/blachlylab/swiftover",
	"license": "GPL-3.0",
	"dependencies": {
		"emsi_containers": "~>0.8.0-alpha.12",
		"dhtslib": { "path": "../dhtslib" }
	},
	"buildRequirements": [ "allowWarnings" ],
        "sourceFiles": [ "../htslib-jblachly/libhts.a" ],
	"lflags": ["-lcurl", "-lbz2", "-llzma", "-lcrypto" ],
	"subConfigurations": {
		"dhtslib": "source-static2"
	},
	"configurations": [
		{
			"name": "splaytree",
			"version": "splay",
			"targetType": "executable"
		},
		{
			"name": "avltree",
			"version": "avl",
			"targetType": "executable"
		}
	]
}
