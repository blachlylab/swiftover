{
	"name": "swiftover",
	"description": "Swift coordinate liftover",
	"authors": ["James S Blachly, MD"],
	"homepage": "http://github.com/blachlylab/swiftover",
	"license": "GPL-3.0",
	"dependencies": {
        "emsi_containers": "~>0.8.0-alpha.12",
        "dhtslib": 	{ "path": "../dhtslib" },
        "dklib":	{ "path": "../dklib" },
        "intervaltree": { "path": "../intervaltree" }
	},
	"buildRequirements": [ "allowWarnings" ],
    "dflags-ldc": [ "" ],
	"lflags": ["-lcurl", "-lbz2", "-llzma"  ],
	"subConfigurations": {
		"dhtslib": "source"
	},
	"configurations": [
		{
			"name": "splaytree",
			"versions": ["splay"],
			"targetType": "executable"
		},
		{
			"name": "avltree",
			"versions": [ "avl" ],
			"targetType": "executable"
		},
        {
            "name": "iitree",
            "versions": [ "iitree" ],
            "targetType": "executable"
        },
        {
            "name": "instrument-splaytree",
            "versions": [ "instrument" , "splay" ],
            "targetType": "executable"
        }
	]
}
